{
  "expo": {
    "name": "Lineus",
    "slug": "Lineus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FAFAFA"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.venepe.phone",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSLocationUsageDescription": "Lineus needs access to your location to find the local phone numbers."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FAFAFA"
      },
      "package": "com.venepe.phone",
      "versionCode": 1,
      "permissions": ["LOCATION"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
